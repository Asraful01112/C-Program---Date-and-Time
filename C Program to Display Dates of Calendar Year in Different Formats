#include <stdio.h>

int isLeap(int year) {
    return ((year % 4 == 0 && year % 100 != 0) || year % 400 == 0);
}

int main() {
    int year;
    int daysInMonth;

    char *months[] = {
        "January", "February", "March", "April",
        "May", "June", "July", "August",
        "September", "October", "November", "December"
    };

    int monthDays[] = {
        31, 28, 31, 30,
        31, 30, 31, 31,
        30, 31, 30, 31
    };

    printf("Enter year: ");
    scanf("%d", &year);

    if (isLeap(year))
        monthDays[1] = 29;

    printf("\nDates of the year %d:\n\n", year);

    for (int m = 0; m < 12; m++) {
        daysInMonth = monthDays[m];

        for (int d = 1; d <= daysInMonth; d++) {

            printf("%02d-%02d-%04d\t", d, m + 1, year);

            printf("%02d/%02d/%04d\t", m + 1, d, year);

            printf("%04d.%02d.%02d\t", year, m + 1, d);

            printf("%02d %s %d", d, months[m], year);

            printf("\n");
        }
    }

    return 0;
}
