#include <stdio.h>

int main() {
    int hours;
    int minutes, seconds;

    printf("Enter hours: ");
    scanf("%d", &hours);

    minutes = hours * 60;
    seconds = hours * 3600;

    printf("\n%d hour(s) = %d minutes\n", hours, minutes);
    printf("%d hour(s) = %d seconds\n", hours, seconds);

    return 0;
}
